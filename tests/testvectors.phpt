<?php if (!extension_loaded('kupyna')) die('skip'); ?>
--FILE--
<?php
$vec = '';
$len = array(512, 1024, 2048, 760, 1536, 0);
for ($i=0; $i<256; ++$i) $vec .= chr($i);

foreach ($len as $l) {
	echo hash('kupyna256', substr($vec, 0, $l / 8)), PHP_EOL;
	echo hash('kupyna384', substr($vec, 0, $l / 8)), PHP_EOL;
	echo hash('kupyna512', substr($vec, 0, $l / 8)), PHP_EOL;
}

$vec = chr(0xFF);
echo hash('kupyna256', $vec), PHP_EOL;
echo hash('kupyna384', $vec), PHP_EOL;
echo hash('kupyna512', $vec), PHP_EOL;
?>
